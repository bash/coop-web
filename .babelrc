{
  "comments": false,
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 ios_saf version"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "external-helpers",
    "transform-class-properties",
    [
      "transform-react-jsx", { "pragma": "h" }
    ]
  ]
}
